         Swal.fire(\n                'Operación exitosa',\n                'El producto se agregó correctamente',\n                'success'\n            );\n        } catch (error) {\n            console.log(error);\n            dispatch( agregarProductoError(true) );\n\n            // Alertar al usuario\n            Swal.fire({\n                icon: 'error',\n                title: 'Hubo un error',\n                text: 'Hubo un error, intenta de nuevo'\n            });\n        }\n    };\n}\n\n\nconst agregarProducto = () => ({\n    type: AGREGAR_PRODUCTO\n});\n\n// Si el producto se guarda en la abse de datos\nconst agregarProductoExito = producto => ({\n    type: AGREGAR_PRODUCTO_EXITO,\n    payload: producto\n});\n\n// Si hubo un error al guardar el producto\nconst agregarProductoError = estado => ({\n    type: AGREGAR_PRODUCTO_ERROR,\n    payload: estado\n});","/Volumes/Backup2/preact/crudproductredux/src/config/axios.js",[],{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","severity":1,"message":"66","line":14,"column":1,"nodeType":"67","endLine":36,"endColumn":2},{"ruleId":"68","severity":1,"message":"69","line":9,"column":8,"nodeType":"70","messageId":"71","endLine":9,"endColumn":21},"no-native-reassign",["72"],"no-negated-in-lhs",["73"],"import/no-anonymous-default-export","Unexpected default export of anonymous function","ExportDefaultDeclaration","no-unused-vars","'clienteAxsion' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]